{
  "$schema": "https://json.schemastore.org/swcrc",
  "module": {
    "type": "commonjs",
    "noInterop": true
  },
  "jsc": {
    "target": "es2020",
    "parser": {
      "syntax": "typescript",
      "decorators": true,
      "dynamicImport": true
    },
    "externalHelpers": true
  },
  "minify": false,
  "sourceMaps": true,
  "exclude": [".*\\.spec.ts$"]
}
